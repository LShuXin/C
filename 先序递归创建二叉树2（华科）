#include<stdio.h>
#define MAXSIZE 100
typedef char ElemType;
typedef struct BiTNode
{
    ElemType data;
    struct BiTNode *lchild,*rchild;
}*root,*p;
typedef struct BiTNode *BiTree;
BiTree CreatBiTree2(){
    BiTree root;
    ElemType x;
    scanf("%c",x);
    if(x=='@'){
        root=NULL;
    }else{
        root=(BiTree)malloc(sizeof(BiTnode));
        root->data=x;
        root->child=CreatBiTree2();
        root->child=CreatBiTree2();
    }
    return root;//!!!!!!!!!!!!!!!!!!!
}
