void SimpleSort(RecType r[],int N){//存放无序记录数组，数组的长度
    int i,j;
    for(i=2;i<=N;i++){
        r[0]=r[i];//第一次未使用监视哨，不能写成r[0].key=r[i].key!!!
        j=i-1;
        while(r[j].key>r[i].key){//r【i】前面的元素不能比它本身大
            r[j+1]=r[j];//边寻找边空出位置，写成了r【j】=r[j+1]!!!!
            j--;
        }
        //执行完毕，插入位置的元素已经到达了他的后面
        //而且j又跑到了插入位置的前面
        r[j+1]=r[0];
    }
}
